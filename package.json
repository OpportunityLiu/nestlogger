{
  "name": "nest-logger",
  "version": "2.1.0",
  "description": "Logger library for Nest apps",
  "author": {
    "name": "Jukka Hell",
    "url": "https://www.maksien.fi"
  },
  "contributors": [
    {
      "name": "Juuso Kosonen"
    }
  ],
  "license": "MIT",
  "keywords": [
    "nest",
    "nestjs",
    "winston",
    "daily-rotate",
    "logger",
    "logging"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jukkahell/nestlogger.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "tslint --fix lib/**/*.ts -p tsconfig.json -c tslint.json",
    "build": "rimraf dist && tsc",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "6.3.2",
    "@nestjs/core": "6.3.2",
    "@nestjs/testing": "6.3.2",
    "event-stream": "4.0.1",
    "merge": "1.2.1",
    "moment": "2.24.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "typescript": "3.5.2",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.9.0"
  },
  "devDependencies": {
    "@types/cli-color": "0.3.29",
    "@types/express": "4.17.0",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/rimraf": "2.0.2",
    "@types/supertest": "2.0.7",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.18.0",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
