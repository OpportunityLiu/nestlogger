{
  "name": "nest-logger",
  "version": "1.4.1",
  "description": "Logger library for Nest apps",
  "author": "Jukka Hell",
  "license": "MIT",
  "keywords": ["nest", "winston", "daily-rotate", "logger", "logging"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "tslint --fix lib/**/*.ts -p tsconfig.json -c tslint.json",
    "build": "rimraf dist && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "5.6.2",
    "@nestjs/core": "5.6.2",
    "@nestjs/testing": "5.6.2",
    "event-stream": "3.3.4",
    "merge": "1.2.1",
    "moment": "2.24.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.4.0",
    "typescript": "3.3.1",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.6.0"
  },
  "devDependencies": {
    "@types/cli-color": "0.3.29",
    "@types/express": "4.16.1",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.21",
    "@types/rimraf": "2.0.2",
    "@types/supertest": "2.0.7",
    "jest": "24.0.0",
    "nodemon": "1.18.9",
    "prettier": "1.16.3",
    "rimraf": "2.6.3",
    "supertest": "3.4.2",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.2",
    "tsconfig-paths": "3.7.0",
    "tslint": "5.12.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
